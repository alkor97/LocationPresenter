{
  "name": "location-presenter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf ./dist && shx rm -rf ./coverage",
    "pretest": "shx test -e node_modules/canvas || shx mv node_modules/canvas-prebuilt node_modules/canvas",
    "test": "jest --coverage",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "compile": "webpack --config webpack.prod.js --optimize-minimize",
    "build": "npm test && npm run compile"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^21.1.10",
    "@types/leaflet": "^1.2.11",
    "canvas-prebuilt": "^1.6.11",
    "css-loader": "^0.28.11",
    "file-loader": "^0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.10",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "shx": "^0.2.2",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.5.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "leaflet": "^1.3.4",
    "leaflet-arrows": "^1.0.0",
    "pixelmatch": "^4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.test\\.ts$",
    "mapCoverage": true
  }
}
